{
  "source": {
    "include": [
      "src/models",
      "src/services",
      "src/controllers"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "exclude": [
      "node_modules"
    ],
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "encoding": "utf8",
    "recurse": true
  }
}